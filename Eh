#include <FastLED.h>
#define NUM_LEDS 134
#define DATA_PIN 11

    CRGB leds[NUM_LEDS];
    void setup(){
     
      FastLED.addLeds<WS2812B, DATA_PIN,  GRB>(leds,NUM_LEDS);
      }
      void loop(){
        for(int i=0; i<NUM_LEDS; i++){
        leds[NUM_LEDS]= NUM_LEDS;
        

     fill_solid(leds, NUM_LEDS, leds[i]= 0x00FFFF);  // Aqua
     FastLED.show();
     delay(1000);
    
    
    fill_solid(leds, NUM_LEDS, leds[i]= 0xCA1F7B);  // Magenta
    FastLED.show();
    delay(1000);
        }
      }
      
    
