#include <FastLed.h>
#define NUM_LEDS 135
#define DATA_PIN 11

    CRGB leds[NUM_LEDS];
    void setup(){
      FastLed.addLeds<WS2812B, DATA_PIN(leds,NUM_LEDS);
      }
      void loop(){
        for(i=0; i<NUM_LEDS; i++){
        leds[i]= CRGB::Dark Cyan;
        FastLED.show();
        delay(55); 
        }
